{
	"name": "RssTodoList Extension",
    "version": "1.2",
	"manifest_version": 2,
	"description": "Send links to RssTodoList - https://rsstodolist.appspot.com/",
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"tabs",
		"https://rsstodolist.appspot.com/"
	]
}

